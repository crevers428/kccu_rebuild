{
  "include": [
    "./pages/api"
    , "config"  
    ],
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "allowJs": true,
    //"checkJs": false,
    "jsx": "preserve",
    "outDir": "./build",
    "rootDir": "./pages",
    "strict": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules"
  ]
}
